{
  "name": "js-quest-blocks",
  "version": "1.0.0",
  "description": "Scratch-like JavaScript learning game for kids",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack:win": "electron-builder --win nsis portable"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "electron": "^37.4.0",
    "electron-builder": "^26.0.12"
  },
  "build": {
    "appId": "com.jsquest.blocks",
    "productName": "JS Quest Blocks",
    "files": [
      "index.html",
      "main.js",
      "package.json"
    ],
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": [
        "nsis",
        "portable"
      ],
      "artifactName": "${productName}-${version}-${arch}.${ext}"
    }
  }
}
